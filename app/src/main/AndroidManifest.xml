<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rine.tabPageSliding" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:name="com.rine.tabPageSliding.App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.rine.tabPageSliding.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.rine.tabPageSliding.NormalTabActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.rine.tabPageSliding.IconTabActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name="com.rine.tabPageSliding.CustomTabActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.rine.tabPageSliding.ViewTabActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.rine.tabPageSliding.WeiboTabActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
